{"components/common/footer":"{{#if banners.bottom}}\n    {{#if settings.data_tag_enabled}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            {{#each (limit banners.bottom_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{else}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            <div class=\"banner\">\n                {{{limit banners.bottom 1}}}\n            </div>\n        </div>\n    {{/if}}\n{{/if}}\n\n\n                    \n<div class=\"footer_main\" style=\"margin-top: 80px;\">\n    \n    <div class=\"footer_column\" style=\"display: flex; align-items: center; justify-content: flex-start; flex-direction: column; height: 100%; position: relative; bottom: 55px;\">\n        <h2></h2>\n        <div class=\"logofooter\">\n            <a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n                {{#if settings.store_logo.image}}\n                    {{#if theme_settings.logo_size '===' 'original'}}\n                    <img class=\"header-logo-image-unknown-size\"\n                        src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                        alt=\"{{settings.store_logo.title}}\"\n                        title=\"{{settings.store_logo.title}}\">\n                    {{else}}\n                    <img class=\"header-logo-image\"\n                        style=\"max-height: 100% !important;\"\n                        src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                        srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n  