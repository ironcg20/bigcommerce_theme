{"components/common/requireness-msg":"<small {{#if required}}class=\"is-required\"{{/if}}>\n    {{#if required}}\n    ({{lang 'common.required'}})\n    {{else}}\n    {{lang 'common.optional'}}\n    {{/if}}\n</small>\n","components/products/options/set-radio":"<div class=\"form-field\" data-product-attribute=\"set-radio\" role=\"radiogroup\" aria-labelledby=\"radio-group-label-{{id}}\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" id=\"radio-group-label-{{id}}\">\n        {{ this.display_name }}:\n\n        {{> components/common/requireness-msg}}\n    </label>\n\n    {{#unless required}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_radio_{{id}}_none\"\n            name=\"attribute[{{id}}]\"\n            value=\"\"\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\n        >\n        <label class=\"form-label\" for=\"attribute_radio_{{id}}_none\">{{lang 'products.none'}}</label>\n    {{/unless}}\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_radio_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n"}
