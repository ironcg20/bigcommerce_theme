{"components/account/edit-account":"<form action=\"{{urls.account.update_action}}\" data-edit-account-form class=\"form\" method=\"post\">\n    {{inject 'firstName' (lang 'forms.validate.account.edit.first_name')}}\n    {{inject 'lastName' (lang 'forms.validate.account.edit.last_name')}}\n    {{inject 'phoneNumber' (lang 'forms.validate.account.edit.phone')}}\n    {{inject 'currentPassword' (lang 'forms.validate.account.edit.password')}}\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-row form-row--half\">\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"account_firstname\">\n                    {{lang 'account.settings.first_name' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"account_firstname\" id=\"account_firstname\" value=\"{{forms.edit_account.first_name}}\">\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"account_lastname\">\n                    {{lang 'account.settings.last_name' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"account_lastname\" id=\"account_lastname\" value=\"{{forms.edit_account.last_name}}\">\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"account_companyname\">\n                    {{lang 'account.settings.company' }}\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"account_companyname\" id=\"account_companyname\" value=\"{{forms.edit_account.company_name}}\">\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"account_phone\">\n                    {{lang 'account.settings.phone' }}\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"account_phone\" id=\"account_phone\" value=\"{{forms.edit_account.phone}}\">\n            </div>\n            {{#each forms.edit_account.fields}}\n                {{{dynamicComponent 'components/common/forms'}}}\n            {{/each}}\n        </div>\n    </fieldset>\n    <div class=\"form-actions\">\n        <button class=\"button button--primary\">{{lang 'account.settings.update_details'}}</button>\n    </div>\n</form>\n","components/common/forms/checkbox":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}\">{{label}}\n        {{#if required}}\n            <small>{{lang 'common.required' }}</small>\n        {{/if}}\n    </label>\n    {{#if checkboxes}}\n        {{#each checkboxes}}\n            <input type=\"checkbox\"\n                   value=\"{{value}}\"\n                   name=\"{{name}}\"\n                   id=\"{{name}}\"\n                   data-input\n                   data-label=\"{{label}}\"\n                   class=\"form-checkbox\"\n                   {{#if checked}}checked{{/if}}\n                   {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n            >\n            <label class=\"form-label {{class}}\" for=\"{{name}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n","components/common/forms/date":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}\">\n        {{label}}\n        {{#if required}}\n            <small>{{lang 'common.required' }}</small>\n        {{/if}}\n    </label>\n    <div class=\"form-row form-row--third\">\n        <div class=\"form-field\">\n            <select class=\"form-select\"\n                    name=\"{{month.name}}\"\n                    id=\"{{id}}_month\"\n                    data-label=\"month\"\n                    data-input\n                    aria-required=\"{{required}}\"\n                    {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n            >\n            {{> components/common/forms/date-options month isRequired=required}}\n            </select>\n        </div>\n        <div class=\"form-field\">\n            <select class=\"form-select\"\n                    name=\"{{day.name}}\"\n                    id=\"{{id}}_day\"\n                    data-label=\"day\"\n                    data-input\n                    aria-required=\"{{required}}\"\n                    {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n            >\n            {{> components/common/forms/date-options day isRequired=required}}\n            </select>\n        </div>\n        <div class=\"form-field\">\n            <select class=\"form-select\"\n                    name=\"{{year.name}}\"\n                    id=\"{{id}}_year\"\n                    data-label=\"year\"\n                    data-input\n                    aria-required=\"{{required}}\"\n                    {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n            >\n            {{> components/common/forms/date-options year isRequired=required}}\n            </select>\n        </div>\n    </div>\n</div>\n","components/common/forms/date-options":"{{#unless this.value}}\n    <option value=\"\">---</option>\n    {{#each this.items}}\n        <option value=\"{{value}}\">{{label}}</option>\n    {{/each}}\n{{else}}\n    {{#unless this.isRequired}}\n        <option value=\"\">---</option>\n    {{/unless}}\n    {{#each this.items}}\n        <option {{#if ../this.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\n    {{/each}}\n{{/unless}}\n","components/common/forms/multiline":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}\n            <small>{{lang 'common.required' }}</small>\n        {{/if}}\n    </label>\n    <textarea name=\"{{name}}\"\n              id=\"{{id}}_input\"\n              data-label=\"{{label}}\"\n              rows=\"{{rows}}\"\n              aria-required=\"{{required}}\"\n              data-input\n              class=\"form-input\"\n              {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n    >\n        {{value}}\n    </textarea>\n</div>\n","components/common/forms/number":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\n    </label>\n    <input type=\"number\"\n           class=\"form-input\"\n           data-label=\"{{label}}\"\n           data-input\n           data-rule=\"lowerThan\"\n           min=\"{{limitfrom}}\"\n           max=\"{{limitto}}\"\n           title=\"{{lang 'forms.range' limitFrom=limitfrom limitTo=limitto}}\"\n           value=\"{{value}}\"\n           id=\"{{id}}_input\"\n           name=\"{{name}}\"\n           aria-required=\"{{required}}\"\n           {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n    >\n</div>\n","components/common/forms/password":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}\n            <small>{{lang 'common.required' }}</small>\n        {{/if}}\n    </label>\n    <input type=\"password\"\n           class=\"form-input\"\n           id=\"{{id}}_input\"\n           data-label=\"{{label}}\"\n           name=\"{{name}}\"\n           value=\"{{value}}\"\n           autocomplete=\"off\"\n           aria-required=\"{{required}}\"\n           {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n    >\n</div>\n","components/common/forms/radio":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\n    <label class=\"form-label\">{{label}}\n        {{#if required}}\n            <small>{{lang 'common.required' }}</small>\n        {{/if}}\n    </label>\n    {{#if radios}}\n        {{#each radios}}\n            <input\n                type=\"radio\"\n                class=\"form-radio\"\n                name=\"{{../name}}\"\n                id=\"{{../name}}_{{@index}}\"\n                value=\"{{value}}\"\n                data-label=\"{{label}}\"\n                data-input\n                aria-required=\"{{required}}\"\n                {{#if checked}}checked{{/if}}\n                {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n            >\n            <label class=\"form-label\" for=\"{{../name}}_{{@index}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n","components/common/forms/select":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}} data-type=\"{{private_id}}\"\n    {{/if}}>\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\n        {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\n    </label>\n    <select class=\"{{class}} form-select\"\n            id=\"{{id}}_select\"\n            data-label=\"{{label}}\"\n            data-input\n            aria-required=\"{{required}}\"\n            name=\"{{name}}\"\n            {{#if private_id}} data-field-type=\"{{private_id}}\" {{/if}}\n    >\n        {{#if options}}\n        <option value=\"\">{{chooseprefix}}</option>\n        {{#each options}}\n        <option {{#if selected}} selected {{/if}} value=\"{{value}}\">{{{label}}}</option>\n        {{/each}}\n        {{/if}}\n    </select>\n</div>\n","components/common/forms/selectortext":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <noscript>\n        <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n            {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\n        </label>\n        <input type=\"text\"\n               id=\"{{id}}_input\"\n               name=\"{{name}}\"\n               value=\"{{value}}\"\n               class=\"form-input\"\n               aria-required=\"{{required}}\"\n               data-label=\"{{label}}\"\n               data-input\n        />\n    </noscript>\n\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <select name=\"{{name}}\"\n            class=\"form-select\"\n            aria-required=\"{{required}}\"\n            id=\"{{id}}_select\"\n            data-label=\"{{label}}\"\n            data-input\n            {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n    >\n        {{#if options}}\n            <option value=\"\">{{chooseprefix}}</option>\n            {{#each options}}\n                <option {{#if selected}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\n            {{/each}}\n        {{/if}}\n    </select>\n</div>\n","components/common/forms/text":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\n    </label>\n    <input type=\"text\"\n           name=\"{{name}}\"\n           id=\"{{id}}_input\"\n           class=\"form-input\"\n           data-label=\"{{label}}\"\n           data-input\n           aria-required=\"{{required}}\"\n           {{#if value}} value=\"{{value}}\"{{/if}}\n           {{#if maxlength}}maxlength=\"{{maxlength}}\"{{/if}}\n           {{#if placeholder}} placeholder=\"{{placeholder}}\"{{/if}}\n           {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n    >\n</div>\n"}
