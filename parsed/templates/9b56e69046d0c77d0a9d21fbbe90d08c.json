{"components/common/requireness-msg":"<small {{#if required}}class=\"is-required\"{{/if}}>\n    {{#if required}}\n    ({{lang 'common.required'}})\n    {{else}}\n    {{lang 'common.optional'}}\n    {{/if}}\n</small>\n","components/products/options/input-file":"<div class=\"form-field\" data-product-attribute=\"input-file\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_file_{{id}}\">\n        {{ this.display_name }}:\n\n        {{> components/common/requireness-msg}}\n    </label>\n\n    <input class=\"form-file\" type=\"file\" id=\"attribute_file_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">\n        {{#if file_size}}\n            {{lang 'products.max_filesize'}}\n            <strong>{{file_size}}{{lang 'products.kilobytes_abbreviation'}}</strong>,\n        {{/if}}\n        {{#if file_types}}\n            {{lang 'products.filetypes'}} <strong>{{file_types}}</strong>\n        {{/if}}\n    </p>\n\n</div>\n"}
