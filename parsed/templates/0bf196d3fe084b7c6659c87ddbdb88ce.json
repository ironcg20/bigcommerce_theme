{"components/common/wishlist-dropdown":"<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action\" data-wishlist-add method=\"post\">\n    <a aria-controls=\"wishlist-dropdown\"\n        aria-expanded=\"false\"\n        class=\"button button--secondary dropdown-menu-button\"\n        data-dropdown=\"wishlist-dropdown\" \n        href=\"#\"\n        style=\"display:flex;align-items:center;justify-content:center;margin-left:0px !important;padding: 15px !important;\"\n    >\n        <span>{{lang 'account.wishlists.add_item'}}</span>\n        <i aria-hidden=\"true\" class=\"icon\">\n            <svg>\n                <use xlink:href=\"#icon-chevron-down\" />\n            </svg>\n        </i>\n    </a>\n    <ul aria-hidden=\"true\" class=\"dropdown-menu\" data-dropdown-content id=\"wishlist-dropdown\" style=\"padding: 0px !important;\">\n        {{#if customer.wishlists}}\n            {{#each customer.wishlists}}\n                <li style=\"width: 100%;\">\n                    <input  style=\"margin: 0px; width: 100%;\"\n                            class=\"button {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\"\n                            formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\n                            type=\"submit\"\n                            value=\"{{this.name}}\"\n                    >\n                </li>\n            {{/each}}\n        {{else}}\n        <li>\n            <input class=\"button\" type=\"submit\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\n        </li>\n        {{/if}}\n        <li>\n            <a data-wishlist class=\"button\" href=\"/wishlist?action=addwishlist&product_id={{product.id}}\">{{lang 'account.wishlists.create_new'}}</a>\n        </li>\n    </ul>\n</form>\n"}
